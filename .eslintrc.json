{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["svelte", "prettier", "import"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.svelte"],
      "parser": "svelte-eslint-parser"
    }
  ],
  "rules": {},
  "ignorePatterns": [
    "bun.lockb",
    "dist/*",
    "src/assets/*",
    "**/*.svg",
    "**/*.html",
    "**/*.md",
    "**/*.json"
  ]
}
